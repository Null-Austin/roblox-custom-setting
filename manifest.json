{
    "manifest_version": 3,
    "name": "Roblox Notes",
    "version": "1.0",
    "description": "Lets you take notes on Roblox games!",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["https://www.roblox.com/*"],
    "content_scripts": [
      {
        "matches": ["https://www.roblox.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  